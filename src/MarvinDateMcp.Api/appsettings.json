{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Security": {
    "ApiKey": "",
    "AllowedOrigins": [
      "https://localhost:5001"
    ]
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowMinutes": 1,
    "QueueLimit": 0
  },
  "RetryPolicy": {
    "RetryCount": 3,
    "BaseDelaySeconds": 2
  },
  "Google": {
    "ApiKey": ""
  },
  "DateService": {
    "HolidayCacheTtlDays": 30,
    "GeocodeCacheTtlDays": 7,
    "HolidayLookaheadDays": 90
  }
}
